  <style>

  .notification {
  background-color:transparent;
  color: white;
  text-decoration: none;
  position: relative;
  display: inline-block;
  border-radius: 2px;
}

.notification .badge {
  position: absolute;
  float:right;
  top:-20px;
  padding: 5px 10px;
  border-radius: 50%;
  background-color: red;
  color: white;
}


  </style>

  <body>

    <nav class="navbar navbar navbar-dark bg-dark" >
       <a class="navbar-brand" style="color:yellow" href="#">BUG_TRACKER</a>
       <div class="navbar-brand">
        <a onclick="move()" class="notification" style="margin-right:30px;">
          <i class="fa fa-bell" id="bell" style="font-size:1em;cursor:pointer"></i>
          <h6 class="badge" ></h6>
        </a>
        <a class="navbar-brand" href="/">Logout</a>
      </div>
    </nav>


    <div style="width:100%;height:100%">

      <div style="float:right;width:100%;height:100%;padding:10px;position:relative;position:fixed">

      <div style="float:right;margin:10px">
       <img src="./img/user.png" class="profile" title=<%=title%> data-toggle="tooltip" style="width:50px;height:50px;display:inline-block">
      </div>
      <h3 style="text-align:center;width:100%;font: italic small-caps bold ">YOUR PROJECTS</h3>

      <div class="chart-container" style="position:relative;height:160vh;width:160vh;float:center;left:auto;right:auto">
       <canvas id="detail"></canvas>
      </div>

     </div>

    <div style="float:left;width:10%;height:1000px;background-color:#212529;padding-top:20px;padding-left:15px;position:fixed">
     <i class="fa fa-home" style="color:yellow;font-size:1.5em">    <a href="/admin?email=<%=email%>" style="text-decoration:none;color:white">Home</a></i>
     <br/>
     <br/>
     <i class='far fa-comments' style="color:yellow;font-size:1.5em">      <a href="chat?email=<%=email%>&name=<%=name%>" style="text-decoration:none;color:white">Discuss</a></i>
     <br/>
     <br/>
     <i class='fas fa-calendar-alt' style="color:yellow;font-size:1.5em">   <a href="/calender?project=&name=<%=name%>&email=<%=email%>&post=" style="text-decoration:none;color:white">Calender</a></i>
     <br/>
     <br/>
     <i class="fa fa-archive"  style="color:yellow;font-size:1.5em">   <a href="/project?email=<%=email%>&name=<%=name%>" style="text-decoration:none;color:white">Project</a></i>
    </div>


</div>

    <script>
    $(function () {
      $(".profile").tooltip({html: true, placement: "left", trigger: "hover"});
    })
    </script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
