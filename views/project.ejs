<div style="width:100%;height:100%">


   <div style="float:right;width:90%;height:100%;">

    <br/>
    <div id="notify" style="display:none;margin-bottom:20px;text-align:center;background-color:green;color:white;font-size:1.2em;margin-right:center;margin-left:center"> Update Successfull </div>
    <div id="notify1" style="display:none;margin-bottom:20px;text-align:center;background-color:green;color:white;font-size:1.2em;margin-right:center;margin-left:center"> Added Successfully </div>

      <button id="new"> New Project </button>

      <div id="project_form" style="background-color:white;z-index:1;display:none;border-radius:3px;box-shadow:5px 5px 5px 5px grey; height:86%;width:35%;float:right;position:absolute;right:5px">

        <div style="background-image: linear-gradient(to top, #f3e7e9 0%, #e3eeff 99%, #e3eeff 100%);height:7%;width:100%;float:right;position:absolute;right:0px">
          <h4 style="margin-left:4%;margin-top:2%">New Project</h4>
        </div>

        <div style="background-color:white;height:92%;width:100%;float:right;position:absolute;top:10%;right:0px;padding:20px">

            <form id="project1">

            <label for="title" style="font-size:1.1em">Project Title</label>
            <br/>
            <input type="text" title="title" id="title"   name="title" style="border-radius:5px;width:100%;font-size:1.2em;border-color:#D3D3D3">

            <br/>
            <br/>

            <div style="float:left;width:45%">
            <label for="owner" style="font-size:1.1em">Owner</label>
            <br/>
            <input type="text" title="owner" id="owner"   name="owner" style="border-radius:5px;width:100%;font-size:1.2em;border-color:#D3D3D3">
            </div>

            <div style="float:right;width:45%">
            <label for="template" style="font-size:1.1em">Template</label>
            <br/>
            <input type="text" title="template" id="template"  name="template" style="border-radius:5px;width:100%;font-size:1.2em;border-color:#D3D3D3">
            </div>

            <br/>
            <br/>
            <br/>
            <br/>


            <div style="float:left;width:43%">
            <label for="start" style="font-size:1.1em">Start Date</label>
            <br/>
            <input type="date" title="start" id="start"   name="start" style="border-radius:5px;width:100%;font-size:1.2em;border-color:#D3D3D3">
            </div>

            <div style="float:right;width:43%">
            <label for="end" style="font-size:1.1em">End Date</label>
            <br/>
            <input type="date" title="end" id="end" name="end"   style="border-radius:5px;width:100%;font-size:1.2em;border-color:#D3D3D3">
            </div>

            <br/>
            <br/>
            <br/>
            <br/>

           <label for="overview" style="font-size:1.1em"> Project Overview</label>
           <textarea name="overview" id="overview"   style="height:150px;width:100%"></textarea>

           <br/>
           <br/>

           <label for="group" style="font-size:1.1em">Group Name</label>
           <br/>
           <input type="text" id="group" name="group"  style="border-radius:5px;width:40%;font-size:1.2em;border-color:#D3D3D3">

           <br/>
           <br/>

           <div style="position:fixed;bottom:0px">
             <input type="submit" id="submit"  value="Add" class="btn btn-primary"   style="margin-right:10px">
             <input type="submit" id="submit1" style="display:none" value="Update" class="btn btn-primary"   style="margin-right:20px">
             <p class="btn btn-light" id="cancel" onclick="clear()" style="border-color:grey;margin-top:12px;" >Cancel</p>
           </div>
          </form>

        </div>

      </div>

      <br/>
      <br/>


    <table id="myTable" style="width:90%;margin-left:auto;margin-right:auto;border-collapse: separate;
      border-spacing: 0 1em;">
     <tr style="text-align:center;cursor:pointer;background-color:#d3d3d3">
      <th id="data"><img src="./img/img.jpg" style="width:20px;height:20px"></th>
      <th id="data">PROJECT_NAME</th>
      <th id="data">%</th>
      <th id="data">OWNER</th>
      <th id="data">STATUS</th>
      <th id="data">MILESTONE</th>
      <th id="data">BUGS</th>
      <th id="data">START DATE</th>
      <th id="data">END DATE</th>
      <th id="data">COMPLETED ON</th>
      <th id="data">TAGS</th>
    </tr>
    <%- include('partials/detail') %>
   </table>
   </div>


   <div style="float:left;width:10%;height:100%;background-color:#212529;padding-top:20px;padding-left:15px">
     <i class="fa fa-home" style="color:yellow;font-size:1.5em">    <a href="/admin?email=<%=email%>" style="text-decoration:none;color:white">Home</a></i>
     <br/>
     <br/>
     <i class='far fa-comments' style="color:yellow;font-size:1.5em">      <a href="chat?email=<%=email%>&name=<%=name%>" style="text-decoration:none;color:white">Discuss</a></i>
     <br/>
     <br/>
     <i class='fas fa-calendar-alt' style="color:yellow;font-size:1.5em">   <a href="/calender?project=&name=<%=name%>&email=<%=email%>&post=" style="text-decoration:none;color:white">Calender</a></i>
     <br/>
     <br/>
     <i class="fa fa-archive"  style="color:yellow;font-size:1.5em">   <a href="/project?email=<%=email%>&name=<%=name%>" style="text-decoration:none;color:white">Project</a></i>
    </div>


</div>

